{
    "name": "Textextractor",
    "description": "extract your text selection and store in local DB",
    "version": "0.1.0",
    "manifest_version": 2,
    "background": {
        "scripts": [
            "./background.js"
        ]
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab", "<all_urls>"]
}